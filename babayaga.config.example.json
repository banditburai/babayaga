{
  "services": [
    {
      "name": "puppeteer",
      "command": "npm",
      "args": ["run", "start:puppeteer-mcp"],
      "healthCheckInterval": 30000
    },
    {
      "name": "cdp",
      "command": "npm",
      "args": ["run", "start:cdp-mcp"],
      "healthCheckInterval": 30000
    }
  ],
  "alternativeConfig": {
    "comment": "Example of connecting to already-running services",
    "services": [
      {
        "name": "puppeteer",
        "url": "ws://localhost:3000",
        "healthCheckUrl": "http://localhost:3000/health",
        "healthCheckInterval": 30000
      },
      {
        "name": "cdp",
        "url": "ws://localhost:3001",
        "healthCheckUrl": "http://localhost:3001/health",
        "healthCheckInterval": 30000
      }
    ]
  },
  "pooledConfig": {
    "comment": "Example with connection pooling for high-performance scenarios",
    "services": [
      {
        "name": "cdp",
        "url": "ws://localhost:3001",
        "useConnectionPool": true,
        "poolConfig": {
          "minConnections": 2,
          "maxConnections": 10,
          "idleTimeout": 300000,
          "acquireTimeout": 5000
        },
        "healthCheckInterval": 30000
      }
    ]
  }
}